<h4>Log In</h4>
<% if flash.alert %>
  <p class="alert">
    <%= flash.alert %>
  </p>
<% end %>
<%= form_with url: :session, id: "login_form" do %>

  <label>ユーザーID</label>
  <input type="text" name="user_id">

  <label>パスワード</label>
  <input type="password" name="password">

  <input type="submit" value="ログイン">
<% end %>