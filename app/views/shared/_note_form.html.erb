<%= render "shared/errors", obj: @note %>
<% statuses = ["public", "draft"] %>
<dl>
    <dt>
      <%= f.label :status %>
    </dt>
    <dd>
      <%= f.select :status, statuses %>
    </dd>
    <dt>
      <%= f.label :key %>
    </dt>
    <dd>
      <%= f.text_field :key %>
    </dd>
    <dt>
      <%= f.label :scale %>
    </dt>
    <dd>
      <%= f.text_field :scale %>
    </dd>
    <dt>
      <%= f.label :memo %>
    </dt>
    <dd>
      <%= f.text_area :memo %>
    </dd>
    <dt>
      <%= f.label :abc %>
    </dt>
    <dd>
      <%= f.text_area :abc %>
    </dd>
  </dl>
  <%= hidden_field_tag :ref, @ref %>
  <%= f.hidden_field :user_id, value: current_user.id %>
  <%= f.hidden_field :tune_id, value: @tune_id %>
