<h4>Result</h4>
<% if @tunes.present? %>
  <dl class="search_result">
    <% @tunes.each do |tune| %>
      <dt><%= link_to tune.name, tune, class: "big" %></dt>
      <dd>
        Rhythm:<%= tune.rhythm %><br />
      </dd>
    <% end %>
  </dl>
<% else %>
  <p>Not Found</p>
<% end %>
<%= paginate @tunes %>
<%= render "shared/search_form" %>